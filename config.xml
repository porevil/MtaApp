<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<widget xmlns="http://www.w3.org/ns/widgets" xmlns:gap="http://phonegap.com/ns/1.0" id="com.tisco.mta" versionCode="10" version="0.1">
  <name>My TISCO Assset</name>
  <description>
        An awesome Asset application.
    </description>
  <author href="http://www.tisco.com" email="natapong@tisco.co.th">natapong</author>

  <platform name="android">
    <preference name="AndroidLaunchMode" value="singleTop"/>
    <preference name="android-minSdkVersion" value="15" />
</platform>

  <plugin name="cordova-plugin-device" spec="~1.1.3"/>
  <plugin name="cordova-plugin-console" spec="~1.0.4"/>
  <plugin name="cordova-plugin-whitelist" spec="~1.3.0"/>
  <plugin name="cordova-plugin-splashscreen" spec="~4.0.0"/>
  <plugin name="cordova-plugin-statusbar" spec="~2.2.0"/>
  <plugin name="ionic-plugin-keyboard" spec="~2.2.1"/>
  <plugin name="cordova-plugin-crosswalk-webview" source="npm"/>
  <plugin name="cordova-plugin-network-information" source="npm"/>

  <icon src="resources\ios\icon\icon-small@3x.png"/>
  <preference name="prerendered-icon" value="true"/>
  <preference name="target-device" value="universal"/>
  <preference name="android-windowSoftInputMode" value="stateAlwaysHidden"/>
